<?xml version="1.0" encoding="utf-8"?>
<manifest manifest-version="0.1" xmlns="http://www.tableau.com/xml/extension_manifest">
  <dashboard-extension id="com.company.advancedgrid" extension-version="1.0.0">
    <default-locale>en_US</default-locale>
    <name resource-id="name"/>
    <description resource-id="description"/>
    <author name="Your Company" email="admin@yourcompany.com" organization="Your Organization" website="https://www.yourcompany.com"/>
    <min-api-version>1.0</min-api-version>
    <source-location>
      <url>https://github.com/vizmetadata/tableau-grid-extension/blob/main/tableau-grid-extension.html</url>
    </source-location>
    <icon>
      
    </icon>
    <permissions>
      <permission>full data</permission>
    </permissions>
    <context-menu>
      <configure-context-menu-item />
    </context-menu>
  </dashboard-extension>
  <resources>
    <resource id="name">
      <text locale="en_US">Advanced Data Grid</text>
    </resource>
    <resource id="description">
      <text locale="en_US">A powerful data grid extension with advanced features including search, sorting, pagination, and export capabilities for Tableau dashboards.</text>
    </resource>
  </resources>
</manifest>